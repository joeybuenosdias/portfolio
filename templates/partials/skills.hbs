<section class="skills-section">
    <h2 class="skills-title">Skills</h2>
    <div class="skills-container">
        {{#each skills}}
            <div class="skill-card">
                <img
                    class="skill-logo"
                    src={{this.vector}}
                />
                <p class="skill-label">{{this.label}}</p>
            </div>
        {{/each}}
    </div>
</section>